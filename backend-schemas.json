        }
    ],
    "label_categories": ["Material", "Part", "Function", "Texture", "Dimension", "Other"],
    "design_categories": ["aerospace", "automotive", "mechanical", "architecture", "industrial", "other"],
    "license_types": ["commercial", "personal", "attribution", "non-commercial"],
    "file_origins": ["original", "modified", "commissioned"],
    "units_supported": ["meters", "millimeters", "inches", "feet"],
    "analysis_statuses": ["pending", "running", "completed", "failed"],
    "payment_statuses": ["completed", "pending", "refunded"],
    "payout_statuses": ["pending", "processing", "completed", "failed"],
    "environment_variables": {
        "database": ["DATABASE_URL"],
        "authentication": ["JWT_SECRET_KEY", "JWT_ACCESS_TOKEN_EXPIRE_MINUTES"],
        "file_storage": ["UPLOAD_PATH", "MAX_UPLOAD_SIZE"],
        "ai_integration": ["OPENAI_API_KEY", "ANTHROPIC_API_KEY", "GEMINI_API_KEY"],
        "cloud_storage": ["AWS_ACCESS_KEY_ID", "AWS_SECRET_ACCESS_KEY", "S3_BUCKET_NAME"]
    }
}
